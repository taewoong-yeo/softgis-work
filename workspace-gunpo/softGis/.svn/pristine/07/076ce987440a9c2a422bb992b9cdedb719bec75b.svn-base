\n\t\t\t\tflex-basis: 100% / ($responsive-column-count / $i);\r\n\t\t\t\tmax-width: 100% / ($responsive-column-count / $i);\r\n\t\t\t}\r\n\r\n\t\t\t@each $name, $px in $responsive-map {\r\n\t\t\t\t@include breakpoint($px) {\r\n\t\t\t\t\t&-#{$name}-#{$i} {\r\n\t\t\t\t\t\tflex-basis: 100% / ($responsive-column-count / $i);\r\n\t\t\t\t\t\tmax-width: 100% / ($responsive-column-count / $i);\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n}","%icon-fa {\r\n\t-moz-osx-font-smoothing: antialiased;\r\n\t-webkit-font-smoothing: antialiased;\r\n\tdisplay: inline-block;\r\n\tfont-style: normal;\r\n\tfont-variant: normal;\r\n\ttext-rendering: auto;\r\n\tline-height: 1;\r\n}\r\n\r\n%icon-fas {\r\n\t@extend %icon-fa;\r\n\r\n\tfont-family: 'Font Awesome 5 Free';\r\n\tfont-weight: 900;\r\n}\r\n\r\n%icon-far {\r\n\t@extend %icon-fa;\r\n\t\r\n\tfont-family: 'Font Awesome 5 Free';\r\n\tfont-weight: 400;\r\n}\r\n\r\n%icon-fab {\r\n\t@extend %icon-fa;\r\n\r\n\tfont-family: \"Font Awesome 5 Brands\";\r\n}\r\n\r\n%icon-bx {\r\n\tfont-family: 'boxicons' !important;\r\n\tfont-weight: normal;\r\n\tfont-style: normal;\r\n\tfont-variant: normal;\r\n\tline-height: 1;\r\n\tdisplay: inline-block;\r\n\ttext-transform: none;\r\n\tspeak: none;\r\n\t-webkit-font-smoothing: antialiased;\r\n\t-moz-osx-font-smoothing: grayscale;\r\n}\r\n\r\n%image {\r\n\tbackground-repeat: no-repeat;\r\n\tbackground-position: center center;\r\n}\r\n\r\n%image-fluid {\r\n\t@extend %image;\r\n\r\n\tbackground-size: contain;\r\n}\r\n\r\n%paragraph {\r\n\tline-height: $default-paragraph-line-height;\r\n}\r\n\r\n%pre {\r\n\twhite-space: pre-wrap;\r\n\tline-height: $default-paragraph-line-height;\r\n}","*, *:before, *:after {\r\n\tbox-sizing: border-box;\r\n\tmargin: 0;\r\n\tpadding: 0;\r\n\tmin-height: 0;\r\n\tmin-width: 0;\r\n}\r\n\r\nhtml, body {\r\n\twidth: 100%;\r\n\t// height: 85%;\r\n\tfont-family: $default-font-family;\r\n\tfont-weight: $default-font-weight;\r\n\tfont-size: $default-font-size;\r\n\tline-heigh