n-center{\r\n    text-align:center;\r\n}\r\n\r\n\r\n\r\n\r\n/************************\r\n common \r\n ************************/\r\nhtml, body{\r\n    width:100%;\r\n    height:100%;\r\n    background:#fdfdfd;\r\n}\r\n\r\n.datepicker-input{\r\n    border:1px solid #dedede;\r\n    box-sizing:border-box;\r\n    width:100px;\r\n    vertical-align:middle;\r\n    padding:3px 10px;\r\n    margin-right:5px;\r\n    border-radius:5px;\r\n}\r\n.datepicker-input[readonly]{\r\n    background-color: #fff;\r\n    font-size: rem(20px);\r\n    width: 130px;\r\n}\r\n\r\n.dashboard-header{\r\n    display:flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    border:1px solid #eee;\r\n    padding: rem(20px);\r\n    background:#f0f0f0;\r\n    margin-bottom:0.5em;\r\n    .dashboard-title{\r\n        display:flex;\r\n        align-items: center;\r\n        \r\n        i{\r\n            margin-top: 4px;\r\n            font-size: rem(28px);\r\n        }\r\n        h2{\r\n            font-size: rem(30px);\r\n            margin-left:.5em;\r\n            color:#152475;\r\n        }\r\n    }\r\n    .dashboard-opt{\r\n        display:flex;\r\n        align-items: center;\r\n        .datepicker-box{\r\n            display:flex;\r\n            align-items: center;\r\n            p{\r\n                margin-right: 15px;\r\n                font-size: rem(24px);\r\n            }\r\n            .calendar-box{\r\n                display:flex;\r\n                span{\r\n                    position:relative;\r\n                    label{\r\n                        position:absolute;\r\n                        top:50%;\r\n                        right:10px;\r\n                        transform: translateY(-50%);\r\n                        font-size: 16px;\r\n                    }\r\n                    &:last-child{\r\n                        display:flex;\r\n                        padding:1px 0;\r\n                        button{\r\n                            background:#03a9f4;\r\n                            color:#fff;\r\n                            border-radius:5px;\r\n                            padding:0 10px;\r\n                            font-size: rem(20px);\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n        }\r\n        .adm-cd{\r\n            display:flex;\r\n            align-items: center;\r\n            &:before{\r\n                content:\"\";\r\n                display:inline-block;\r\n                width:1px;\r\n                height:15px;\r\n                background: #0000ff;\r\n                margin:0 .75em;\r\n            }\r\n            display:flex;\r\n            select{\r\n                border:1px solid #dedede;\r\n                box-sizing: border-box;\r\n                width: 140px;\r\n                vertical-align: middle;\r\n                padding: 3px 10px;\r\n                border-radius: 5px;\r\n                background-color:#fff;\r\n                font-size: rem(18px);\r\n            }\r\n        }\r\n    }\r\n}\r\n// body#dashboard{\r\n//     overflow:hidden;\r\n// }\r\n// body#dashboard .ctn{\r\n//     overflow:auto;\r\n//     position:relative;\r\n//     width:100%;\r\n//     height:100%;\r\n// }\r\n// body#dashboard .main{\r\n//     width:100%;\r\n//     height:100%;\r\n// }\r\nh1, h2, h3, h4, h5, h6{\r\n    font-weight:500;\r\n}\r\nh1{\r\n    font-size:20px;\r\n}\r\nh2{\r\n    font-size:18px;\r\n}\r\nh3{\r\n    font-size:16px;\r\n}\r\nh4{\r\n    font-size:14px;\r\n}\r\nh5{\r\n    font-size:12px;\r\n}\r\nh6{\r\n    font-size:10px;\r\n}\r\n\r\n.al{\r\n    text-align:left;\r\n}\r\n.ar{\r\n    text-align:right;\r\n}\r\n.ar{\r\n    text-align:right;\r\n}\r\n   \r\nem.point{\r\n    color:$point-color;\r\n}\r\n\r\n.mt-10{\r\n    margin-top:10px;\r\n}\r\n.mt-15{\r\n    margin-top:15px;\r\n}\r\n.mt-20{\r\n    margin-top:20px;\r\n}\r\n.mt-30{\r\n    margin-top:30px;\r\n}\r\n\r\n/************************\r\n modal\r\n ************************/\r\n\r\n.dashboard-modal{\r\n    display:none;\r\n    background:rgba(0,0,0,0.75);\r\n    position:fixed;\r\n    top:0;\r\n    left:0;\r\n    width:100%;\r\n    height:100%;\r\n    z-index:999;\r\n    .dashboard-modal-box{\r\n    \tdisplay:none;\r\n        position: absolute;\r\n        top:50%;\r\n        left:50%;\r\n        transform: translate(-50%, -50%);\r\n        max-width:1080px;\r\n        width:100%;\r\n        overflow:hidden;\r\n        border-radius:10px;\r\n        background:#fff;\r\n        .dashboard-modal-btn{ \r\n            text-align:right;\r\n            padding:5px 15px;\r\n            button{\r\n                font-size:20px;\r\n            }\r\n        } \r\n        .dashboard-modal-header{\r\n            position:relative;\r\n            padding: $layout-space-15;\r\n            box-sizing:border-box;\r\n            border-top:1px solid #f0f0f0;\r\n            border-bottom:1px solid #f0f0f0;\r\n            background:#152475;\r\n            p{ \r\n                font-size:18px;\r\n                color:#fff;\r\n            }\r\n        }\r\n        .dashboard-modal-body{\r\n            padding:15px;\r\n            box-sizing:border-box;\r\n        }\r\n    }\r\n}\r\n\r\n\r\n/************************\r\n header\r\n ************************/\r\nheader{\r\n    width:calc(100% - 70px);\r\n    position:fixed;\r\n    left:70px;\r\n    height:55px;\r\n    background:#000;\r\n    padding:0 30px;\r\n    z-index:98;\r\n    h1{\r\n        color:#fff;\r\n        font-size:18px;\r\n        line-height:55px;\r\n        span{\r\n            display:inline-block;\r\n            &:before{\r\n                content:\"\";\r\n                display:inline-block;\r\n                width:1px;\r\n                height:15px;\r\n                background:#fff;\r\n                vertical-align:middle;\r\n                margin:0 15px;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n/************************\r\n nav \r\n ************************/\r\n// nav {\r\n//     position: fixed;\r\n//     top: 0;\r\n//     left: 0;\r\n//     z-index:99;\r\n//     width: 70px;\r\n//     height: 100%;\r\n//     background: #fff;\r\n//     .nav-title{\r\n//         p{\r\n//             height: 55px;\r\n//             line-height: 55px;\r\n//             text-align: center;\r\n//             background: #495060;\r\n//             color: #fff;\r\n//             font-weight: 500;\r\n//             letter-spacing: 1px;\r\n//         }\r\n//     }\r\n//     ul{\r\n//         height:100%;\r\n//         border-right:1px solid #dedede;\r\n//         li {\r\n//             border-bottom: 1px solid #f0f0f0;\r\n//             text-align: center;\r\n//             a {\r\n//                 display: block;\r\n//                 color: #333;\r\n//                 font-size: 14px;\r\n//                 padding: 10px 0;\r\n//             }\r\n//         }\r\n//     }\r\n// }\r\n\r\n/************************\r\n main\r\n ************************/\r\n.main{\r\n    height:100%;\r\n}\r\n.ctn{\r\n    overflow-x: auto;\r\n    width: 100%;\r\n    height: 100%;\r\n    position: relative;\r\n}\r\n\r\n\r\n.dashboard-container{\r\n    width:100%;\r\n    overflow-x:auto;\r\n    overflow-y:auto;\r\n    // background:#eef0f5;\r\n    #dashboard-main{\r\n        min-width:1500px;\r\n        padding:15px;\r\n    }\r\n}\r\n#main{\r\n    min-width:1500px;\r\n    padding:$layout-space-15;    \r\n}\r\n\r\n.dashboard-information{\r\n\tmargin-bottom:10px;\r\n\tborder-radius:5px;\r\n\toverflow:hidden;\r\n\tbackground:#006bd8;\r\n\tpadding:$layout-space-15;    \r\n\tp{\r\n\t\tfont-size:20px;\r\n\t\tcolor:#fff;\r\n\t\tem{\r\n\t\t\tcolor:#F1F241;\r\n\t\t\tfont-weight:500;\r\n\t\t\tfont-size:18px;\r\n\t\t}\r\n\t}\r\n}\r\n\r\n.card-box{\r\n    overflow:hidden;\r\n    border-radius:5px;\r\n    border:1px solid #dee2e6;\r\n    padding:5px 10px;\r\n    background-color:#fff;\r\n    min-height:130px;\r\n    & + &{\r\n        margin-top:$layout-space-15;\r\n    }\r\n    .ca